<?import javafx.scene.control.*?>
<MenuButton mnemonicParsing="false" text="MenuButton" xmlns="http://javafx.com/javafx/18"
            xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="splitstree6.contextmenus.algorithmnode.AlgorithmNodeContextMenuController">

    <contextMenu>
        <ContextMenu fx:id="contextMenu">
            <items>
                <MenuItem fx:id="editMenuItem" mnemonicParsing="false" text="Edit"/>
                <SeparatorMenuItem mnemonicParsing="false"/>
                <MenuItem fx:id="runMenuItem" mnemonicParsing="false" text="Run"/>
                <MenuItem fx:id="stopMenuItem" mnemonicParsing="false" text="Stop"/>
                <SeparatorMenuItem mnemonicParsing="false"/>
                <MenuItem fx:id="duplicateMenuItem" mnemonicParsing="false" text="Duplicate"/>
                <MenuItem fx:id="deleteMenuItem" mnemonicParsing="false" text="Delete"/>
            </items>
        </ContextMenu>
    </contextMenu>
</MenuButton>
