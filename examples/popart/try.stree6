#nexus [SplitsTree6]

BEGIN SPLITSTREE6;
DIMENSIONS nDataNodes=8 nAlgorithms=5;
PROGRAM version='SplitsTree6 Community Edition';
WORKFLOW creationDate='1648978482984'; [Sun Apr 03 11:34:42 CEST 2022]
END; [SPLITSTREE6]

[
Methods:
Analysis was performed using SplitsTree6 6.0.0_alpha (Huson and Bryant 2006).
The original input consisted of 7 taxa and 7 DNA sequences of length 56.
After removal of 2 taxa, the input consisted of 5 taxa and 5 DNA sequences of length 56.
The Hamming Distances method (Hamming 1950) was used (default options) so as to obtain a 5 x 5 distance matrix.
The Neighbor Net method (Bryant & Moulton 2004) was used (default options) so as to obtain 6 splits, cyclic.
The Show Splits method was used (default options) so as to obtain a Splits Network visualization (Dress & Huson 2004).

References:
Bryant & Moulton 2004: D. Bryant and V. Moulton. Neighbor-net: An agglomerative method for the construction of phylogenetic networks. Molecular Biology and Evolution, 21(2):255– 265, 2004.
Dress & Huson 2004: A.W.M. Dress and D.H. Huson, Constructing splits graphs, IEEE/ACM Transactions on Computational Biology and Bioinformatics 1(3):109-115, 2004.
Hamming 1950: Hamming, Richard W. Error detecting and error correcting codes. Bell System Technical Journal. 29 (2): 147–160. MR 0035935, 1950.
Huson and Bryant 2006: D.H. Huson and D. Bryant. Application of phylogenetic networks in evolutionary studies. Molecular Biology and Evolution, 23:254–267, 2006.
]

BEGIN TAXA;
TITLE 'Input Taxa';
DIMENSIONS ntax=7;
TAXLABELS
	[1] 'seq_1'
	[2] 'seq_2'
	[3] 'seq_3'
	[4] 'seq_4'
	[5] 'seq_5'
	[6] 'seq_6'
	[7] 'seq_7'
;
[DISPLAYLABELS;]
[TAXINFO;]
END; [TAXA]

BEGIN TRAITS;
TITLE 'Input Traits';
DIMENSIONS [nTax=7] nTraits=11;
FORMAT labels=yes missing=? separator=Comma;
TRAITLABELS
 'Europe' 'Asia' 'Africa' 'Australia' 'America' 'a' 'b' 'c' 'd' 'e' 'f';
MATRIX
[1] 'seq_1'  0,0,0,3,3,1,1,33,4,3,1
[2] 'seq_2'  10,5,0,6,0,2,444,5,6,7,1
[3] 'seq_3'  0,0,0,3,5,3,0,0,0,0,0
[4] 'seq_4'  0,0,0,4,2,4,3,5,6,7,8
[5] 'seq_5'  4,0,10,0,0,5,1,1,1,11,1
[6] 'seq_6'  0,0,0,7,3,6,1,1,111,1,1
[7] 'seq_7'  0,0,5,0,0,7,3,4,5,6,1
;
END; [TRAITS]

BEGIN ALGORITHM;
TITLE 'Taxa Filter';
NAME 'Taxa Filter';
OPTIONS
	DisabledTaxa = 'seq_1' 'seq_2';
END; [ALGORITHM]

BEGIN TAXA;
TITLE 'Working Taxa';
LINK ALGORITHM = 'Taxa Filter';
DIMENSIONS ntax=5;
TAXLABELS
	[1] 'seq_3'
	[2] 'seq_4'
	[3] 'seq_5'
	[4] 'seq_6'
	[5] 'seq_7'
;
[DISPLAYLABELS;]
[TAXINFO;]
END; [TAXA]

BEGIN TRAITS;
TITLE 'Working Traits';
LINK ALGORITHM = 'Taxa Filter';
DIMENSIONS [nTax=5] nTraits=11;
FORMAT labels=yes missing=? separator=Comma;
TRAITLABELS
 'Europe' 'Asia' 'Africa' 'Australia' 'America' 'a' 'b' 'c' 'd' 'e' 'f';
MATRIX
[1] 'seq_3'  0,0,0,3,3,1,1,33,4,3,1
[2] 'seq_4'  10,5,0,6,0,2,444,5,6,7,1
[3] 'seq_5'  0,0,0,3,5,3,0,0,0,0,0
[4] 'seq_6'  0,0,0,4,2,4,3,5,6,7,8
[5] 'seq_7'  4,0,10,0,0,5,1,1,1,11,1
;
END; [TRAITS]

BEGIN CHARACTERS;
TITLE 'Input Characters';
LINK ALGORITHM = 'Taxa Filter';
DIMENSIONS ntax=7 nchar=56;
FORMAT
	datatype='DNA' missing=n gap=- symbols="acgt" labels=left transpose=no interleave=no;
MATRIX
seq_1 atatacggggtta---ttaga----aaaatgtgtgtgtgttttttttttcatgtgg
seq_2 atatac--ggata---ttaca----agaatctatgtctgctttctttttcatgtgg
seq_3 atatacggggata---ttata----agaatgtgtgtgtgttttttttttcatgtgg
seq_4 atatacggggata---gtagt----aaaatgtgtgtgtgttttttttttcatgtga
seq_5 atatacggggata---gtagg----aaaatgtgtgtgtgttttttttttcatgtgc
seq_6 atatacggggata---gtagc----aaaatgtgtgtgtgttttttttttcatgtgt
seq_7 atatacggggata---gtaga----aaaatgtgtgtgtgttttttttttcatgtga
;
END; [CHARACTERS]

BEGIN ALGORITHM;
TITLE 'Input Data Filter';
LINK CHARACTERS = 'Input Characters';
NAME 'Characters Taxa Filter';
END; [ALGORITHM]

BEGIN CHARACTERS;
TITLE 'Working Characters';
LINK ALGORITHM = 'Input Data Filter';
DIMENSIONS ntax=5 nchar=56;
FORMAT
	datatype='DNA' missing=n gap=- symbols="acgt" labels=left transpose=no interleave=yes;
MATRIX
seq_3 atatacggggata---ttata----agaatgtgtgtgtgttttttttttcatgtgg
seq_4 atatacggggata---gtagt----aaaatgtgtgtgtgttttttttttcatgtga
seq_5 atatacggggata---gtagg----aaaatgtgtgtgtgttttttttttcatgtgc
seq_6 atatacggggata---gtagc----aaaatgtgtgtgtgttttttttttcatgtgt
seq_7 atatacggggata---gtaga----aaaatgtgtgtgtgttttttttttcatgtga

;
END; [CHARACTERS]

BEGIN ALGORITHM;
TITLE 'Hamming Distances';
LINK CHARACTERS = 'Working Characters';
NAME 'Hamming Distances';
OPTIONS
	Normalize = 'true';
END; [ALGORITHM]

BEGIN DISTANCES;
TITLE 'Distances';
LINK ALGORITHM = 'Hamming Distances';
DIMENSIONS ntax=5;
FORMAT labels=left diagonal triangle=Both;
MATRIX
[1] 'seq_3'   0 0.102041 0.102041 0.102041 0.081633
[2] 'seq_4'   0.102041 0 0.040816 0.040816 0.020408
[3] 'seq_5'   0.102041 0.040816 0 0.040816 0.040816
[4] 'seq_6'   0.102041 0.040816 0.040816 0 0.040816
[5] 'seq_7'   0.081633 0.020408 0.040816 0.040816 0
;
END; [DISTANCES]

BEGIN ALGORITHM;
TITLE 'Neighbor Net';
LINK DISTANCES = 'Distances';
NAME 'Neighbor Net';
OPTIONS
	InferenceAlgorithm = 'BlockPivot',
	UsePreconditioner = 'true';
END; [ALGORITHM]

BEGIN SPLITS;
TITLE 'Splits';
LINK ALGORITHM = 'Neighbor Net';
DIMENSIONS ntax=5 nsplits=6;
FORMAT labels=no weights=yes confidences=no;
PROPERTIES fit=100 cyclic;
CYCLE 1 3 4 2 5;
MATRIX
[1, size=1] 	 0.07142857 	 1,
[2, size=2] 	 0.01020408 	 1 3 4,
[3, size=1] 	 0.02040816 	 1 2 4 5,
[4, size=2] 	 0.01020408 	 1 5,
[5, size=1] 	 0.02040816 	 1 2 3 5,
[6, size=1] 	 0.01020408 	 1 3 4 5,
;
END; [SPLITS]

BEGIN ALGORITHM;
TITLE 'Show Splits';
LINK SPLITS = 'Splits';
NAME 'Show Splits';
OPTIONS
	View = 'SplitsNetwork';
END; [ALGORITHM]

BEGIN VIEW;
TITLE 'SplitsNetwork';
LINK ALGORITHM = 'Show Splits';
NAME 'SplitsNetwork';
INPUT 'SPLITS';
OPTIONS
	Diagram = 'Outline',
	Orientation = 'Rotate90Deg',
	Rooting = 'None',
	ZoomFactor = 1,
	FontScaleFactor = 1,
	RootAngle = 160,
	OutlineFill = 0xccccccff,
	ShowConfidence = 'false',
	ActiveTraits = '*All*';
END; [VIEW]
