<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<beast beautitemplate='Standard' beautistatus=''
       namespace="beast.core:beast.evolution.alignment:beast.evolution.tree.coalescent:beast.core.util:beast.evolution.nuc:beast.evolution.operators:beast.evolution.sitemodel:beast.evolution.substitutionmodel:beast.evolution.likelihood"
       required="" version="2.4">


    <data
            id="Huon-languages"
            dataType="standard"
            name="alignment">
        <sequence id="seq_borong" taxon="borong" totalcount="2"
                  value=""/>
        <sequence id="seq_burum" taxon="burum" totalcount="2"
                  value=""/>
        <sequence id="seq_dedua" taxon="dedua" totalcount="2"
                  value=""/>
        <sequence id="seq_kate" taxon="kate" totalcount="2"
                  value=""/>
        <sequence id="seq_komba" taxon="komba" totalcount="2"
                  value=""/>
        <sequence id="seq_kube" taxon="kube" totalcount="2"
                  value=""/>
        <sequence id="seq_mape" taxon="mape" totalcount="2"
                  value=""/>
        <sequence id="seq_mesem" taxon="mesem" totalcount="2"
                  value=""/>
        <sequence id="seq_mindik" taxon="mindik" totalcount="2"
                  value=""/>
        <sequence id="seq_nabak" taxon="nabak" totalcount="2"
                  value=""/>
        <sequence id="seq_ono" taxon="ono" totalcount="2"
                  value=""/>
        <sequence id="seq_selepet" taxon="selepet" totalcount="2"
                  value=""/>
        <sequence id="seq_timbe" taxon="timbe" totalcount="2"
                  value=""/>
        <sequence id="seq_tobo" taxon="tobo" totalcount="2"
                  value=""/>
        <userDataType id="StandardData.0" spec="beast.evolution.datatype.StandardData" ambiguities="" nrOfStates="2"/>
    </data>


    <map name="Uniform">beast.math.distributions.Uniform</map>
    <map name="Exponential">beast.math.distributions.Exponential</map>
    <map name="LogNormal">beast.math.distributions.LogNormalDistributionModel</map>
    <map name="Normal">beast.math.distributions.Normal</map>
    <map name="Beta">beast.math.distributions.Beta</map>
    <map name="Gamma">beast.math.distributions.Gamma</map>
    <map name="LaplaceDistribution">beast.math.distributions.LaplaceDistribution</map>
    <map name="prior">beast.math.distributions.Prior</map>
    <map name="InverseGamma">beast.math.distributions.InverseGamma</map>
    <map name="OneOnX">beast.math.distributions.OneOnX</map>


    <run id="mcmc" spec="MCMC" chainLength="10000000" preBurnin="10000">
        <state id="state" storeEvery="5000">
            <tree id="Tree.t:Huon-languages" name="stateNode">
                <taxonset id="TaxonSet.Huon-languages" spec="TaxonSet">
                    <alignment idref="Huon-languages"/>
                </taxonset>
            </tree>
            <parameter id="proportionInvariant.s:Huon-languages" lower="0.0" name="stateNode" upper="1.0">0.0
            </parameter>
            <parameter id="mutationRate.s:Huon-languages" name="stateNode">1.0</parameter>
            <parameter id="gammaShape.s:Huon-languages" name="stateNode">1.0</parameter>
            <parameter id="birthRate.t:Huon-languages" name="stateNode">1.0</parameter>
        </state>

        <init id="RandomTree.t:Huon-languages" spec="beast.evolution.tree.RandomTree" estimate="false"
              initial="@Tree.t:Huon-languages" taxa="@Huon-languages">
            <populationModel id="ConstantPopulation0.t:Huon-languages" spec="ConstantPopulation">
                <parameter id="randomPopSize.t:Huon-languages" name="popSize">1.0</parameter>
            </populationModel>
        </init>

        <distribution id="posterior" spec="util.CompoundDistribution">
            <distribution id="prior" spec="util.CompoundDistribution">
                <distribution id="YuleModel.t:Huon-languages" spec="beast.evolution.speciation.YuleModel"
                              birthDiffRate="@birthRate.t:Huon-languages" tree="@Tree.t:Huon-languages"/>
                <prior id="YuleBirthRatePrior.t:Huon-languages" name="distribution" x="@birthRate.t:Huon-languages">
                    <Uniform id="Uniform.1" name="distr" upper="Infinity"/>
                </prior>
                <prior id="GammaShapePrior.s:Huon-languages" name="distribution" x="@gammaShape.s:Huon-languages">
                    <Exponential id="Exponential.0" name="distr">
                        <parameter id="RealParameter.0" estimate="false" name="mean">1.0</parameter>
                    </Exponential>
                </prior>
                <prior id="PropInvariantPrior.s:Huon-languages" name="distribution"
                       x="@proportionInvariant.s:Huon-languages">
                    <Uniform id="Uniform.2" name="distr"/>
                </prior>
            </distribution>
            <distribution id="likelihood" spec="util.CompoundDistribution" useThreads="true">
                <distribution id="treeLikelihood.Huon-languages" spec="ThreadedTreeLikelihood" data="@Huon-languages"
                              tree="@Tree.t:Huon-languages">
                    <siteModel id="SiteModel.s:Huon-languages" spec="SiteModel" gammaCategoryCount="4"
                               mutationRate="@mutationRate.s:Huon-languages"
                               proportionInvariant="@proportionInvariant.s:Huon-languages"
                               shape="@gammaShape.s:Huon-languages">
                        <substModel id="MutationDeathModel.s:Huon-languages" spec="MutationDeathModel">
                            <parameter id="deathprob.s:Huon-languages" estimate="false" name="deathprob">0.1</parameter>
                            <frequencies id="freqs.s:Huon-languages" spec="Frequencies">
                                <parameter id="RealParameter.1" dimension="2" name="frequencies">1.0 0.0</parameter>
                            </frequencies>
                        </substModel>
                    </siteModel>
                    <branchRateModel id="StrictClock.c:Huon-languages"
                                     spec="beast.evolution.branchratemodel.StrictClockModel">
                        <parameter id="clockRate.c:Huon-languages" estimate="false" name="clock.rate">1.0</parameter>
                    </branchRateModel>
                </distribution>
            </distribution>
        </distribution>

        <operator id="FixMeanMutationRatesOperator" spec="DeltaExchangeOperator" delta="0.75" weight="2.0">
            <parameter idref="mutationRate.s:Huon-languages"/>
            <weightvector id="weightparameter" spec="parameter.IntegerParameter" estimate="false" lower="0" upper="0">
                391
            </weightvector>
        </operator>

        <operator id="proportionInvariantScaler.s:Huon-languages" spec="ScaleOperator"
                  parameter="@proportionInvariant.s:Huon-languages" scaleFactor="0.5" weight="0.1"/>

        <operator id="gammaShapeScaler.s:Huon-languages" spec="ScaleOperator" parameter="@gammaShape.s:Huon-languages"
                  scaleFactor="0.5" weight="0.1"/>

        <operator id="YuleBirthRateScaler.t:Huon-languages" spec="ScaleOperator" parameter="@birthRate.t:Huon-languages"
                  scaleFactor="0.75" weight="3.0"/>

        <operator id="YuleModelTreeScaler.t:Huon-languages" spec="ScaleOperator" scaleFactor="0.5"
                  tree="@Tree.t:Huon-languages" weight="3.0"/>

        <operator id="YuleModelTreeRootScaler.t:Huon-languages" spec="ScaleOperator" rootOnly="true" scaleFactor="0.5"
                  tree="@Tree.t:Huon-languages" weight="3.0"/>

        <operator id="YuleModelUniformOperator.t:Huon-languages" spec="Uniform" tree="@Tree.t:Huon-languages"
                  weight="30.0"/>

        <operator id="YuleModelSubtreeSlide.t:Huon-languages" spec="SubtreeSlide" tree="@Tree.t:Huon-languages"
                  weight="15.0"/>

        <operator id="YuleModelNarrow.t:Huon-languages" spec="Exchange" tree="@Tree.t:Huon-languages" weight="15.0"/>

        <operator id="YuleModelWide.t:Huon-languages" spec="Exchange" isNarrow="false" tree="@Tree.t:Huon-languages"
                  weight="3.0"/>

        <operator id="YuleModelWilsonBalding.t:Huon-languages" spec="WilsonBalding" tree="@Tree.t:Huon-languages"
                  weight="3.0"/>

        <logger id="tracelog" fileName="Huon-languages.log" logEvery="1000" model="@posterior" sanitiseHeaders="true"
                sort="smart">
            <log idref="posterior"/>
            <log idref="likelihood"/>
            <log idref="prior"/>
            <log idref="treeLikelihood.Huon-languages"/>
            <log id="TreeHeight.t:Huon-languages" spec="beast.evolution.tree.TreeHeightLogger"
                 tree="@Tree.t:Huon-languages"/>
            <log idref="proportionInvariant.s:Huon-languages"/>
            <log idref="mutationRate.s:Huon-languages"/>
            <log idref="gammaShape.s:Huon-languages"/>
            <log idref="YuleModel.t:Huon-languages"/>
            <log idref="birthRate.t:Huon-languages"/>
        </logger>

        <logger id="screenlog" logEvery="1000">
            <log idref="posterior"/>
            <log id="ESS.0" spec="util.ESS" arg="@posterior"/>
            <log idref="likelihood"/>
            <log idref="prior"/>
        </logger>

        <logger id="treelog.t:Huon-languages" fileName="$(tree).trees" logEvery="1000" mode="tree">
            <log id="TreeWithMetaDataLogger.t:Huon-languages" spec="beast.evolution.tree.TreeWithMetaDataLogger"
                 tree="@Tree.t:Huon-languages"/>
        </logger>

    </run>

</beast>
